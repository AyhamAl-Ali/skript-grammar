{
    "name": "skript-grammar",
    "displayName": "Skript",
    "description": "The grammar source for all official Skript highlighters.",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "skript",
            "aliases": ["Skript", "skript"],
            "extensions": [".sk"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "skript",
            "scopeName": "source.skript",
            "path": "./syntaxes/skript.json"
        }]
    }
}